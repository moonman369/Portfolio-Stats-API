{
  "version": 2,
  "builds": [{ "src": "app.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/",
      "dest": "/app.js"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/api/v1/leetcode/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/api/v1/github/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/api/v1/refresh/(.*)",
      "dest": "/app.js"
    }
  ]
}
